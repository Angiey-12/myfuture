{
  "name": "myfuture",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:es5": "ng serve --configuration=es5",
    "start:fake": "concurrently --kill-others \"ng serve --configuration=fake\" \"npm run fake-api\"",
    "start:es5+fake": "concurrently --kill-others \"ng serve --configuration=es5+fake\" \"npm run fake-api\"",
    "build": "ng build --prod=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "fake-api": "json-server fake-api/db.json --routes fake-api/routes.json --no-cors=true --watch",
    "sf:move": "if exist ..\\Public\\app\\index.cshtml (del /f ..\\ResourcePackages\\Bootstrap\\MVC\\Views\\Layouts\\Bootstrap.SiteLayout.cshtml 2>nul && del /f ..\\ResourcePackages\\Bootstrap\\MVC\\Views\\Layouts\\Bootstrap.SiteLayout.LoggedOut.cshtml 2>nul && move ..\\Public\\app\\index.cshtml ..\\ResourcePackages\\Bootstrap\\MVC\\Views\\Layouts\\Bootstrap.SiteLayout.cshtml && copy ..\\ResourcePackages\\Bootstrap\\MVC\\Views\\Layouts\\Bootstrap.SiteLayout.cshtml ..\\ResourcePackages\\Bootstrap\\MVC\\Views\\Layouts\\Bootstrap.SiteLayout.LoggedOut.cshtml)",
    "sf:build": "ng build --prod=true && npm run sf:move",
    "sf:watch": "concurrently --kill-others \"npm run sf:move:watch\" \"ng build --watch --prod=true\"",
    "sf:move:watch": "npm-watch sf:move"
  },
  "watch": {
    "sf:move": {
      "patterns": [
        "../Public/*"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.6",
    "@angular/cdk": "~9.0.0-rc.5",
    "@angular/common": "~9.0.0-rc.6",
    "@angular/compiler": "~9.0.0-rc.6",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~9.0.0-rc.6",
    "@angular/localize": "~9.0.0-rc.6",
    "@angular/material": "^9.0.0-rc.5",
    "@angular/platform-browser": "~9.0.0-rc.6",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.6",
    "@angular/router": "~9.0.0-rc.6",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "@progress/kendo-angular-charts": "^4.1.3",
    "@swimlane/ngx-charts": "^13.0.2",
    "bodybuilder": "^2.2.20",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "hammerjs": "^2.0.0",
    "jw-bootstrap-switch-ng2": "^2.0.5",
    "moment": "^2.24.0",
    "ng2-nouislider": "^1.8.2",
    "nouislider": "^14.1.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "@progress/kendo-angular-common": "^1.0.0",
    "@progress/kendo-angular-intl": "^2.0.0",
    "@progress/kendo-angular-l10n": "^2.0.0",
    "@progress/kendo-drawing": "^1.1.2",
    "@progress/kendo-angular-popup": "^3.0.0",
    "@progress/kendo-theme-default": "latest"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.7",
    "@angular/cli": "^9.0.0-rc.6",
    "@angular/compiler-cli": "~9.0.0-rc.6",
    "@angular/language-service": "~9.0.0-rc.6",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concurrently": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-watch": "^0.6.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.6.4"
  }
}
